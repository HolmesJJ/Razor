    "build:style": "gulp --gulpfile ./packages/theme/gulpfile.js default --color",
    "dev:style": "gulp --gulpfile ./packages/theme/gulpfile.js watch --color --watch",
    "dev": "webpack-dev-server --config build/webpack.dev.js",
    "dev:start": "npm run build:style && webpack-dev-server --config build/webpack.dev.js",
    "test:unit": "jest",
    "eslint": "eslint packages/**/*.vue --fix"
  },
